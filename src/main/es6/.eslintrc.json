{
  "env": {
    "es6": true,
    "browser": true
  },
  "globals": {
    "fetch": true,
    "moment": false
  },
  "rules": {
    "require-jsdoc": 0,
    "max-len": [
      1,
      180,
      4,
      {
        "ignoreComments": true,
        "ignoreUrls": true
      }
    ]
  },
  "parserOptions": {
    "sourceType": "module"
  }
}
